{
	"id": "Thing.json",
	"title": "Thing",
	"description": "The most generic type of item.",
	"format": "http://fibo.sdo-fibo.appspot.com/Thing",
	"media": {"type": "application/json;profile=http://fibo.sdo-fibo.appspot.com/Thing"},
	"type": "object",
	"properties": {
		"additionalType": {
			"type": "array",
			"items": {
				"type": "string",
				"format": "uri",
				"links": [{
					"rel": "http://schema.org/additionalType",
					"href": "{+$}",
					"linkSource": 2
				}]
			}
		},
		"alternateName": {
			"type": "array",
			"items": {"$ref": "#/definitions/alternateName"}
		},
		"description": {
			"type": "array",
			"items": {"$ref": "#/definitions/description"}
		},
		"disambiguatingDescription": {
			"type": "array",
			"items": {"$ref": "#/definitions/disambiguatingDescription"}
		},
		"image": {
			"type": "array",
			"items": {"$ref": "#/definitions/image"}
		},
		"mainEntityOfPage": {
			"type": "array",
			"items": {"$ref": "#/definitions/mainEntityOfPage"}
		},
		"name": {"$ref": "#/definitions/name"},
		"potentialAction": {"$ref": "Action.json#/definitions/possibleRefArray"},
		"sameAs": {
			"type": "array",
			"items": {
				"type": "string",
				"format": "uri",
				"links": [{
					"rel": "http://schema.org/sameAs",
					"href": "{+$}",
					"linkSource": 2
				}]
			}
		},
		"url": {
			"type": "string",
			"format": "uri"
		}
	},
	"links": [{
		"rel": "self",
		"href": "{+url}"
	}],
	"definitions": {
		"array": {
			"type": "array",
			"items": {"$ref": "#"}
		},
		"possibleRef": {
			"oneOf": [
				{"$ref": "#"},
				{
					"type": "string",
					"format": "uri",
					"links": [{
						"rel": "full",
						"href": "{+$}"
					}]
				}
			]
		},
		"possibleRefArray": {
			"oneOf": [
				{
					"type": "string",
					"format": "uri",
					"links": [{
						"rel": "full",
						"href": "{+$}"
					}]
				},
				{
					"type": "array",
					"items": {"$ref": "#/definitions/possibleRef"}
				}
			]
		},
		"alternateName": {
			"title": "Alternate Name",
			"description": "An alias for the item.",
			"type": "string"
		},
		"description": {
			"title": "Description",
			"description": "A description of the item.",
			"type": "string"
		},
		"disambiguatingDescription": {
			"title": "Disambiguating Description",
			"description": "A sub property of description. A short description of the item used to disambiguate from other, similar items. Information from other properties (in particular, name) may be necessary for the description to be useful for disambiguation.",
			"type": "string"
		},
		"image": {
			"title": "Image",
			"description": "An image of the item. This can be a URL or a fully described ImageObject.",
			"anyOf": [
				{"$ref": "ImageObject.json#/definitions/possibleRef"},
				{
					"type": "string",
					"format": "uri"
				}
			]
		},
		"mainEntityOfPage": {
			"title": "Main Entity of Page",
			"description": "Indicates a page (or other CreativeWork) for which this thing is the main entity being described. See background notes for details. Inverse property: mainEntity.",
			"anyOf": [
				{"$ref": "CreativeWork.json#/definitions/possibleRef"},
				{
					"type": "string",
					"format": "uri"
				}
			]
		},
		"name": {
			"title": "Name",
			"description": "The name of the item.",
			"type": "string"
		}
	}
}