	"definitions": {
		"array": {
			"type": "array",
			"items": {"$ref": "#"}
		},
		"possibleRef": {
			"oneOf": [
				{"$ref": "#"},
				{
					"type": "string",
					"format": "uri",
					"links": [{
						"rel": "full",
						"href": "{+$}"
					}]
				}
			]
		},
		"possibleRefArray": {
			"oneOf": [
				{
					"type": "string",
					"format": "uri",
					"links": [{
						"rel": "full",
						"href": "{+$}"
					}]
				},
				{
					"type": "array",
					"items": {"$ref": "#/definitions/possibleRef"}
				}
			]
		},
		"arrivalPlatform": {
			"title": "Arrival Platform",
			"description": "The platform where the train arrives.",
			"type": "string"
		},
		"arrivalTime": {
			"title": "Arrival Time",
			"description": "The expected arrival time.",
			"type": "string",
			"format": "date-time"
		},
		"departurePlatform": {
			"title": "Departure Platform",
			"description": "The platform from which the train departs.",
			"type": "string"
		},
		"departureTime": {
			"title": "Departure Time",
			"description": "The expected departure time.",
			"type": "string",
			"format": "date-time"
		},
		"provider": {
			"title": "Provider",
