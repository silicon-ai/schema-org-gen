		},
		"trainName": {
			"title": "Train Name",
			"description": "The name of the train (e.g. The Orient Express).",
			"type": "string"
		},
		"trainNumber": {
			"title": "Train Number",
			"description": "The unique identifier for the train.",
			"type": "string"
		}
